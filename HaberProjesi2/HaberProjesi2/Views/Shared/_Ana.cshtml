+
@{
    Layout = null;
}

@using HaberProjesi2.MultiLanguage
@{
    Layout = null;
}


<!DOCTYPE HTML>
<html>

<head>
    <meta charset="utf-8">
    <title>Index - küresel Haberler</title>
    <link href="/Content/Home/css/common.css" rel="stylesheet" media="all" />
    <link href="/Content/Home/css/style.css" rel="stylesheet" media="all" />
    <link href="/Content/Home/css/skin.css" rel="stylesheet" media="all" />
    <link rel="icon" href="/Content/Home/images/favicon.ico" type="image/x-icon">
    <link rel="icon" href="/Content/Home/images/favicon.ico" type="image/x-icon">
    <link rel="shortcut icon" href="/Content/Home/images/favicon.ico" type="image/x-icon" />
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
    <script>!window.jQuery && document.write('<script src="/Content/Home/js/jquery-1.7.1.min.js"><\/script>')</script>
    <script src="/Content/Home/js/jquery-ui.min.js"></script>
    <script src="/Content/Home/js/jquery.ad-gallery.pack.js"></script>
    <script src="/Content/Home/js/jquery.anythingslider.min.js"></script>
    <script src="/Content/Home/js/jquery.colorbox-min.js"></script>
    <script src="/Content/Home/js/script.js"></script>

    @RenderSection("head", required: false);
    <!--[if lt IE 9]>
    <script src="/Content/js/html5.js"></script>
    <![endif]-->
</head>
<body class="two_sidebars">
    <!--[if lte IE 8]><div id="ie_message"><div class="wrapper"><a href="http://windows.microsoft.com/en-US/internet-explorer/products/ie/home?ocid=ie6_countdown_bannercode"><img src="images/banner_ie.png" alt="You are using an outdated browser. For a faster, safer browsing experience, upgrade for free today."/></a></div></div><![endif]-->
    <div id="wrapper">
        <!-- header -->
        <header>
            <div id="above_header">
                <div class="inner">
                    <div class="region">
                        <div id="block_header_time" class="block">
                            <p>
                                Today:
                                <time datetime="2018-12-07" class="today">@DateTime.Now</time>
                                Last Update:
                                <time datetime="2018-12-07" class="last_update">3:54 AM ET</time>
                            </p>
                        </div>
                        <div id="block_header_login" class="block">
                            <p>
                                <a href="/Kullanici/Login">Oturum Ac</a> | <a href="~/Kullanici/Index">Kayıt ol</a> | <a href="/Kullanici/Cikis">Cikis Yap</a>


                            </p>
                        </div>
                    </div>
                </div>
            </div>
            <div id="header">
                <div class="wrapper">
                    @*<h1 id="logo"><a href="index.html" title="Home"><img src="/Content/Home/images/logo.png" alt="Global News" /></a></h1>*@
                    <div class="region">
                        <div id="block-search-form" class="block">
                            <form method="post" action="/Home/index">
                                <div class="form-type-textfield">
                                    <input type="text" class="form-text" name="txtAra" maxlength="128" size="15" value="" placeholder="Arama" />
                                </div>
                                <div class="form-actions">
                                    <input type="submit" class="form-submit" value="" />
                                </div>
                            </form>
                        </div>
                        <div id="block_header_rss" class="block">
                            <p>
                                <a>
                                    @Html.ActionLink("Turkce", "Change", "Language", new { LanguageAbbrevation = "tr", returnurl = this.Request.RawUrl }, null)

                                </a> |

                                <a>
                                    @Html.ActionLink("Engilash", "Change", "Language", new { LanguageAbbrevation = "en", returnurl = this.Request.RawUrl }, null)


                                </a>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
            <div id="under_header">
                <div class="region">
                    <div id="block_main_menu" class="block">
                        <div class="inner">
                            <nav>
                                <ul>
                                    <li class="first active"><a href="/Home"><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.AnaSayfa</span></a></li>
                                    <li><a href="/Home"><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Sayfalar</span></a></li>
                                    <li><a href="/Matbaaci"><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Matbaaci</span></a></li>
                                    <li><a href="/Galeri"><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Gallery</span></a></li>
                                    <li><a href=""><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Dunya</span></a></li>
                                    <li><a href=""><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Turkiye</span></a></li>
                                    <li><a href=""><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Ticaret</span></a></li>
                                    <li><a href=""><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Spor</span></a></li>
                                    <li><a href=""><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Eglenceler</span></a></li>

                                    <li><a href=""><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Teknoloji</span></a></li>
                                    <li><a href=""><span class="bg"></span></a></li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                    <div id="block_secondary_menu" class="block">
                        <div class="inner">
                            <ul>
                                <li class="first active"><a href="index.html">Kırmızı</a></li>
                                <li><a href="index_blue.html">Mavi</a></li>
                                <li><a href="index_orange.html">Orange</a></li>
                                <li><a href="index_green.html">Yesil</a></li>
                                <li><a href="index_grey.html">Gri</a></li>
                                <li><a href="index_dark_blue.html">KoyuMavi</a></li>
                                <li><a href="index_dark_grey.html">KoyuGri</a></li>
                                <li><a href="index_fixed_layout.html">Sabit Duzen</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>

        </header>
        <!-- end header -->

        @RenderBody();
        <div id="scroll_to_top_wrapper">
            <div class="wrapper"><a href="#" id="scroll_to_top">Scroll To Top</a></div>
        </div>
        <!-- footer -->
        <footer>
            <div class="region">
                <div id="block_footer_menu" class="block">
                    <nav>
                        <ul>
                            <li class="first active"><a href="/home"><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.AnaSayfa</span></a></li>
                            <li><a href="/home"><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Sayfalar</span></a></li>
                            <li><a href="/Matbaaci"><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Matbaaci</span></a></li>
                            <li><a href="/Galeri"><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Gallery</span></a></li>
                            <li><a href="#"><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Dunya</span></a></li>
                            <li><a href="#"><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Turkiye</span></a></li>
                            <li><a href="#"><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Ticaret</span></a></li>
                            <li><a href="#"><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Spor</span></a></li>
                            <li><a href="#"><span class="bg">@HaberProjesi2.MultiLanguage.Kaynak.Eglenceler</span></a></li>

                            <li><a href="/Home/Login">Admin Girisi</a></li>


                        </ul>
                    </nav>
                </div>
                <div id="block_copyright" class="block">
                    <p>Telif Hakkı &copy; 2018 Proje Ödevi. Tüm Hakları Saklıdır.</p>
                </div>
            </div>
        </footer>
        <!-- end footer -->
    </div>
</body>

</html>
<meta name="viewport" content="width=device-width" />
<title>Ana</title>
