<body class="login-page">
    @using (Html.BeginForm("Autorize", "Home", FormMethod.Post))
    {
        @Html.AntiForgeryToken()
        <div id="maincontainer" class="clearfix">

            <!-- ======= Header start ======= -->
            <header>
                <div class="navbar navbar-fixed-top">
                    <div class="navbar-inner">
                        <div class="container-fluid">
                            <div class="row-fluid">

                                <div class="span6">
                                    @*<a href="#" class="logo"></a>*@
                                </div>



                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <!-- ======= Header end ======= -->

            <div id="formContainer">
                <form method="post" action="/Home/Login/">
                    <div class="logo">
                        @*<img src="/Content/Admin/img/logo-bigger.png" alt="">*@
                    </div>
                    <div class="line">
                        @Html.EditorFor(model => model.Mail, new { htmlAttributes = new { @class = "input100", @type = "text", @name = "Kullaniciadi", @value = "", @placeHolder = "E_Postenizi Giriniz" } })
                        <i class="icon-user"></i>
                        @Html.ValidationMessageFor(model => model.Mail)

                    </div>
                    <div class="line">
                        @Html.EditorFor(model => model.Sifre, new { htmlAttributes = new { @class = "simple_input", @type = "password", @name = "Parola", @value = "", @placeHolder = "Parola" } })
                        <a data-toggle="modal" href="#lostpw" class="forget field_notice">Parolami Unuttun</a>
                        <i class="icon-lock"></i>
                        @Html.ValidationMessageFor(model => model.Sifre)

                    </div>
                    <button class="btn btn-primary" type="submit">Giris Yap</button>

                </form>
                <div class="clear"></div>
            </div>

            <div class="modal hide fade" id="lostpw">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">×</button>
                    <h3>Forgot your password?</h3>
                </div>
                <div class="modal-body">
                    <form class="form-inline">
                        <input type="text" class="simple_input inline" placeholder="Enter your e-mail adress here and press submit">
                    </form>
                </div>
                <div class="modal-footer">
                    <a href="#" class="btn" data-dismiss="modal">Close</a>
                    <a href="#" class="btn btn-danger inline">Submit</a>
                </div>
            </div>


        </div>
    }
    <!-- ======= Main container end ======= -->
