@model IEnumerable<HaberProjesi2.Models.Haber>


@{
    Layout = null;
}


<aside id="block_front_slider" class="block">
    <div class="content ad-gallery">
        <div class="ad-image-wrapper"></div>
        <div class="ad-controls"></div>
        <div class="ad-nav">
            <div class="ad-thumbs">
                <ul class="ad-thumb-list">

                    @foreach (var item in Model)
                    {

                        <li><a href="@item.ResimYol"><img src="@item.KucukResimYol" title="@item.Baslik" alt="@item.Ozet" /></a></li>

                    }

                </ul>
            </div>
        </div>
    </div>
</aside>

